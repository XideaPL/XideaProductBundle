{% extends xidea_template('xidea_product', 'product_main') %}
{% block bundle_title %}
    <h1 class="page-header">{{ 'product_create.title'|trans }}</h1>
{% endblock %}
{% block bundle_main %}
    {% block product_main %}
        <div class="box product-box">
            <div class="box-header">
                <h3 class="box-title">{{ 'product_create.title'|trans }}</h3>
            </div>
            <!-- /.box-header -->
            <div class="box-body">
                {% block product_form %}
                    {{ include(xidea_template('xidea_product', 'product_form')) }}
                {% endblock %}
            </div>
            <!-- /.box-body -->
        </div>
        <!-- /.box -->
    {% endblock %}
{% endblock %}
